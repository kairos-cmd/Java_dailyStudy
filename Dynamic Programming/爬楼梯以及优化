class Solution {
    public int climbStairs(int n) {
        //减小空间复杂度O(1)
        if(n <=1 ){
            return 1;
        }
        int item1 = 1;
        int item2 = 1;
        int item3 = 0; //变量定义在循环之外
        for(int i = 2; i <= n; i++){ //从第二级楼梯开始
            item3 = item1 + item2;
            item1 = item2;
            item2 = item3;  
        }
        return item3;
    }
}
